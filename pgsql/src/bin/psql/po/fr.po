msgstr "acquérir des statistiques concernant la base de données"

#: sql_help.h:102
msgid "ANALYZE [ VERBOSE ] [ table [ (column [, ...] ) ] ]"
msgstr "ANALYZE [ VERBOSE ] [ table [ (colonne [, ...] ) ] ]"

#: sql_help.h:105 sql_help.h:449
